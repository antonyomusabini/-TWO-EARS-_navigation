<launch>
  <node pkg="kemar_teleop" type="kemar_keyboard" name="kemar_keyboard_teleop"  output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="1.0" type="double"/>
    <remap from="kemar_keyboard_teleop/cmd_vel" to="cmd_vel"/>
  </node>
</launch>
